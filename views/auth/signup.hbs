<form method="post" action="/auth/signup" class="form">

    <label for="name">name</label>
    <input class="input" type="text" id="name" name="name" value="test name">
    <label for="nickname">nickname</label>
    <input class="input" type="text" id="nickname" name="nickname" value="nickname">
    <label for="email">email</label>
    <input class="input" type="email" id="email" name="email" value="test@email.com">
    <label for="password">password</label>
    <input class="input" type="password" id="password" name="password" value="12345">
    <button class="btn">signup</button>
</form>
<p>
    Already got an account ? <a href="/auth/signin">signin</a>
</p>