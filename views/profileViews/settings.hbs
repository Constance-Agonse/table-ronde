<div id="profile-nav-bar">
    {{> profileNavBar}}

</div>


<div class="profile-right-block">
    <div id="partials" class="partial-login-logout">
        {{> profileLoginNavBar}}
    </div>
    <div class="skills-block">
        {{#each userInfo}}
        <p class="user-infos"> <span class="boldy"> Name: </span>{{this.name}}</p>
        <p class="user-infos"> <span class="boldy"> Nickname: </span>{{this.nickname}}</p>
        <p class="user-infos"> <span class="boldy"> Email: </span>{{this.email}}</p>
        {{/each}}
        <p class="user-infos"><span class="boldy">Hours: </span>{{userHours}}</p>
    </div>
</div>